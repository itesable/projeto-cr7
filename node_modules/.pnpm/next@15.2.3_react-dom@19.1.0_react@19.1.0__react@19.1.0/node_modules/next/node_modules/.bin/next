#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/next@15.2.3_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/next/dist/bin/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/next@15.2.3_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/next/dist/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/next@15.2.3_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/next/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/next@15.2.3_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/next@15.2.3_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/next/dist/bin/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/next@15.2.3_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/next/dist/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/next@15.2.3_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules/next/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/next@15.2.3_react-dom@19.1.0_react@19.1.0__react@19.1.0/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/next" "$@"
else
  exec node  "$basedir/../../dist/bin/next" "$@"
fi
