#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/tsup@8.5.0_jiti@1.21.7_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules/tsup/dist/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/tsup@8.5.0_jiti@1.21.7_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules/tsup/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/tsup@8.5.0_jiti@1.21.7_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/tsup@8.5.0_jiti@1.21.7_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules/tsup/dist/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/tsup@8.5.0_jiti@1.21.7_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules/tsup/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/tsup@8.5.0_jiti@1.21.7_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules:/home/lucas/clone-cr7/30-06-teste-pnpm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
